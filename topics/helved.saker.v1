apiVersion: kafka.nais.io/v1
kind: Topic
metadata:
  name: saker.v1
  namespace: helved
  labels:
    team: helved
spec:
  pool: {{kafkapool}}
  config:
    cleanupPolicy: compact
    partitions: 3
  acl:
    - team: helved
      application: abetal
      access: readwrite
    - team: helved
      application: peisschtappern
      access: read
    - team: helved
      application: helved
      access: write
