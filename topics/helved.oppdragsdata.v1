apiVersion: kafka.nais.io/v1
kind: Topic
metadata:
  name: oppdragsdata.v1
  namespace: helved
  labels:
    team: helved
spec:
  pool: {{kafkapool}}
  config:
    cleanupPolicy: compact,delete
    retentionHours: 336
    partitions: 3
  acl:
    - team: helved
      application: urskog
      access: write
    - team: helved
      application: peisschtappern
      access: read
    - team: helved
      application: vedskiva
      access: readwrite
