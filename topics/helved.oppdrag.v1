apiVersion: kafka.nais.io/v1
kind: Topic
metadata:
  name: oppdrag.v1
  namespace: helved
  labels:
    team: helved
spec:
  pool: {{ kafka.pool }}
  config:
    cleanupPolicy: compact
    partitions: 3
  acl:
    - team: helved
      application: abetal
      access: readwrite
    - team: helved
      application: urskog
      access: readwrite
